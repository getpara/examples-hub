{
  "name": "examples-hub",
  "type": "module",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "yarn": ">=4.0.0"
  },
  "devDependencies": {
    "@playwright/test": "1.52.0",
    "@tailwindcss/postcss": "4.1.10",
    "@types/node": "^22.15.21",
    "dotenv": "^17.1.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  },
  "scripts": {
    "install:all": "tsx scripts/install-all-dependencies.ts",
    "build:all": "tsx scripts/build-all-projects.ts",
    "lint:all": "tsx scripts/lint-all-projects.ts",
    "typecheck:all": "tsx scripts/typecheck-all-projects.ts",
    "clean:node_modules": "find . -name 'node_modules' -type d -prune -exec rm -rf {} +",
    "clean:dist": "find . -path '*/node_modules' -prune -o \\( -name 'dist' -o -name 'build' -o -name '.next' -o -name '.output' -o -name 'coverage' \\) -type d -print -exec rm -rf {} +",
    "clean": "yarn clean:node_modules && yarn clean:dist",
    "deps:check": "tsx scripts/update-para-dependencies.ts --check-only",
    "deps:update": "tsx scripts/update-para-dependencies.ts",
    "deps:update:diff": "tsx scripts/update-para-dependencies.ts --diff-only",
    "test:all": "tsx e2e/scripts/runAllTests.ts --sequential",
    "test:all:parallel": "tsx e2e/scripts/runAllTests.ts",
    "test:diff": "tsx e2e/scripts/runAllTests.ts --diff-only",
    "test:parallel:diff": "tsx e2e/scripts/runAllTests.ts --diff-only",
    "test:react-vite": "tsx e2e/scripts/runAllTests.ts react-vite",
    "test:react-vite:email-password": "tsx e2e/scripts/runAllTests.ts react-vite email-password",
    "test:react-vite:email-passkey": "tsx e2e/scripts/runAllTests.ts react-vite email-passkey",
    "test:react-vite:phone-password": "tsx e2e/scripts/runAllTests.ts react-vite phone-password",
    "test:react-vite:phone-passkey": "tsx e2e/scripts/runAllTests.ts react-vite phone-passkey",
    "test:react-nextjs": "tsx e2e/scripts/runAllTests.ts react-nextjs",
    "test:react-nextjs:email-password": "tsx e2e/scripts/runAllTests.ts react-nextjs email-password",
    "test:react-nextjs:email-passkey": "tsx e2e/scripts/runAllTests.ts react-nextjs email-passkey",
    "test:react-nextjs:phone-password": "tsx e2e/scripts/runAllTests.ts react-nextjs phone-password",
    "test:react-nextjs:phone-passkey": "tsx e2e/scripts/runAllTests.ts react-nextjs phone-passkey",
    "test:vue": "tsx e2e/scripts/runAllTests.ts vue",
    "test:svelte": "tsx e2e/scripts/runAllTests.ts svelte",
    "test:node": "tsx e2e/scripts/runAllTests.ts node",
    "act:web-ci": "act -W .github/workflows/web-ci.yml",
    "act:web-ci:pr": "act pull_request -W .github/workflows/web-ci.yml",
    "act:deps": "act workflow_dispatch -W .github/workflows/update-para-dependencies.yml",
    "dev:setup": "yarn clean && yarn install:all && yarn build:all",
    "ci:local": "yarn install:all && yarn lint:all && yarn typecheck:all && yarn build:all && yarn test:all:parallel",
    "quality": "yarn lint:all && yarn typecheck:all"
  },
  "packageManager": "yarn@4.9.2"
}
