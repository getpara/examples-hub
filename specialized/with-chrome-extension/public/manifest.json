{
  "manifest_version": 3,
  "name": "Para Chrome Extension",
  "version": "0.0.0",
  "description": "A Chrome extension showing using Para SDK to interact with Para.",
  "action": {
    "default_title": "Para Extension",
    "default_icon": {
      "16": "icons/icon-16.png",
      "48": "icons/icon-48.png",
      "128": "icons/icon-128.png"
    }
  },
  "icons": {
    "16": "icons/icon-16.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "permissions": [
    "tabs",
    "storage",
    "webRequest",
    "sessions",
    "activeTab",
    "scripting",
    "webNavigation",
    "cookies",
    "unlimitedStorage"
  ]
}
