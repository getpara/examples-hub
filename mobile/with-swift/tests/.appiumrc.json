{
  "server": {
    "port": 4723,
    "path": "/",
    "driver": {
      "xcuitest": {
        "webkit-debug-proxy-port": 5400
      }
    }
  },
  "capabilities": {
    "platformName": "ios",
    "appium:automationName": "xcuitest",
    "appium:deviceName": "iPhone 16 Pro",
    "appium:platformVersion": "18.2",
    "appium:bundleId": "com.usecapsule.example.swift",
    "appium:simulatorEnrolledBiometrics": true,
    "appium:autoAcceptAlerts": true,
    "appium:showXcodeLog": true,
    "appium:derivedDataPath": "${HOME}/wda-derived-data",
    "appium:newCommandTimeout": 600,
    "appium:useNewWDA": true,
    "appium:simulator": true,
    "appium:noReset": true,
    "appium:processArguments": {
      "args": ["a", "b"],
      "env": {
        "PARA_API_KEY": "${PARA_API_KEY}",
        "PARA_ENVIRONMENT": "${PARA_ENVIRONMENT}"
      }
    }
  },
  "automationProtocol": "webdriver",
  "baseUrl": "http://localhost:4723",
  "logLevel": "info",
  "framework": "jest"
} 